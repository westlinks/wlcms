{
    "name": "westlinks/wlcms",
    "description": "Professional Content Management System for Laravel - WordPress-level functionality for SaaS applications",
    "keywords": ["laravel", "cms", "content-management", "westlinks"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "West Links",
            "email": "developers@westlinks.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/framework": "^11.0|^12.0",
        "intervention/image-laravel": "^1.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Westlinks\\Wlcms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Westlinks\\Wlcms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Westlinks\\Wlcms\\WlcmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}